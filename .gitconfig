[alias]
    st = status
    ci = commit
    co = checkout
    lola = log --graph --decorate=full\n--pretty=oneline --abbrev-commit --all
    logf = "!echo \"Remember to add -S<string>\" ; git log --color -p --source --all"
[user]
    name = olemorud
    email = ole.kristian.morud@cern.ch
[color]
    ui = true
    diff = auto
    status = auto
[core]
    editor = vim --clean
[credential]
    helper = manager-core
[push]
    default = simple
[branch]
    autosetuprebase = always
[init]
	defaultBranch = master
